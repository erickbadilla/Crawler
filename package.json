{
  "name": "scraper",
  "version": "0.0.1",
  "description": "A Web Scraper Project.",
  "type": "module",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/hpp": "^0.2.2",
    "@types/jest": "~29.2",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.4",
    "@types/node": "~18",
    "@types/user-agents": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "esbuild": "^0.17.5",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "install": "^0.13.0",
    "jest": "~29.3",
    "morgan": "^1.10.0",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "tslint": "^5.1.0",
    "tslint-import-group-ordering": "^2.1.2",
    "tsutils": "~3.21",
    "typescript": "~4.9"
  },
  "scripts": {
    "start": "node build/src/server.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "test": "jest --coverage",
    "prettier": "prettier --config .prettierrc --write ./src/**",
    "test:watch": "jest --watch",
    "build:minify": "npm run build:release && esbuild ./build/src/server.js --bundle --platform=node --minify --outfile=./build/minify/index.min.js",
    "build:windows": "npm run build:minify && set NODE_ENV=production && pkg -t win-x64 ./build/minify/index.min.js -o ./dist/windows/scraper --env NODE_ENV=production",
    "build:mac:x64": "npm run build:minify && pkg -t macos-x64 ./build/minify/index.min.js -o ./dist/mac/scraperX64",
    "build:mac:arm": "npm run build:minify && pkg -t macos-arm64 ./build/minify/index.min.js -o ./dist/mac/scraperARM"
  },
  "author": "Erick Badilla",
  "dependencies": {
    "@types/compression": "^1.7.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21",
    "playwright": "^1.29.2",
    "tslib": "~2.4",
    "user-agents": "^1.0.1260",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1",
    "zod": "^3.20.2"
  },
  "volta": {
    "node": "18.12.1"
  }
}
